<?xml version="1.0" encoding="utf-8"?>
<ParameterList>
  <Parameter name="xml_file_format" type="string" value="DICe_xml_calibration_file"/>
  <Parameter name="image_folder" type="string" value="../images/"/>
  <Parameter name="image_file_prefix" type="string" value="white_dot_"/>
  <Parameter name="image_file_extension" type="string" value=".jpeg"/>
